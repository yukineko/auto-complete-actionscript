(defvar ac-source-as3
  '((candidates 
     . (lambda () 
	 (all-completions ac-target 
			  (append nil 
				  as3-reversed-keywords
				  as3-classname
				  as3-packagenameall
				  as3-sprite-methods
				  )))))
  "Sources for AS3 keywords.")

(add-hook 'actionscript-mode-hook
	  '(lambda () (auto-complete-mode 1)
	     (make-local-variable 'ac-sources)
	     (setq ac-sources '(
				ac-source-abbrev
				ac-source-words-in-buffer
				ac-source-as3))
	     nil))
(provide 'auto-complete-actionscript)
